NAME := fsbext
SRCFILES := @srcdir@/src/fsbext.c

prefix := @prefix@
exec_prefix := @exec_prefix@
bindir := @bindir@

CFLAGS += -I@srcdir@/src 
CFLAGS += -Wno-format 
CFLAGS += @configure_flags@ 
CFLAGS += -O

all:	$(NAME)

install: $(NAME)
	cp $(NAME) $(bindir)

$(NAME): $(OBJECTS)
	gcc ${CFLAGS} -o $(NAME) $(SRCFILES)

.PHONY: clean 
clean:
	rm -f $(NAME) $(OBJECTS)
